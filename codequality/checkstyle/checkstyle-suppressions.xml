<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress files="[\\/].*(package-info)\.java" checks="RegexpOnFilename"/>

    <suppress files=".*[\\/]enums([\\/])?.*" checks="NoWhitespaceBefore"/>
    <suppress files=".*[\\/]enums([\\/])?.*" checks="AtclauseOrder"/>
    <suppress files=".*[\\/]enums([\\/])?.*" checks="JavadocParagraph"/>
    <suppress files=".*[\\/]enums([\\/])?.*" checks="JavadocTagContinuationIndentation"/>
    <suppress files=".*[\\/]enums([\\/])?.*" checks="LineLength"/>

    <suppress files=".*[\\/]client([\\/])?.*" checks="AtclauseOrder"/>
    <suppress files=".*[\\/]client([\\/])?.*" checks="JavadocParagraph"/>
    <suppress files=".*[\\/]client([\\/])?.*" checks="JavadocTagContinuationIndentation"/>
    <suppress files=".*[\\/]client([\\/])?.*" checks="LineLength"/>

    <suppress files=".*[\\/]constant([\\/])?.*" checks="InterfaceIsType"/>
</suppressions>

